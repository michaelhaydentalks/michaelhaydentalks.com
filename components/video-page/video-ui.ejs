      .form-group {
        display: grid;
        grid-auto-flow: row;
        row-gap: 10px;
        margin: 3rem auto 0 auto;
      }

      .form-group:first-of-type {
        margin: 1rem auto 0 auto;
      }

      input,
      textarea {
        background: var(--background-2);
        font-size: 1rem;
        border: 0;
        border-radius: 4px;
        box-shadow: 0 0 0 2px var(--dark);
        padding: 1rem;
        font-family: 'Fira Code', 'Courier New', monospace, "Font Awesome 5 Free";
        color: var(--text);
