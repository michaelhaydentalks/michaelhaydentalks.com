        font-family: 'Fira Code', 'Courier New', monospace, "Font Awesome 5 Free";
        font-style: italic;
      }

      input:focus::placeholder,
      textarea:focus::placeholder {
        color: transparent;
      }

      label {
        font-weight: 800;
        background: -webkit-linear-gradient(60deg, var(--crimson), var(--purple));
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        display: inline-block;
        margin-right: auto;
